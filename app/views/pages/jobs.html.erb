    <div class="span10 offset1">
      <table class="table table-striped">
      <thead>
        <tr>
          <th>City</th>
          <th>Title</th>
          <th>Link</th>
          <th>Location</th>
        </tr>
      </thead>
      <tbody>   
        
        <% unless @posts.empty? %>
          <% @posts.each do |post| %>
            <%= raw(post) %>
          <% end %>
        <% else %>
          <p>No results found!</p>
        <% end %>
      </tbody>
      <table>
    </div>